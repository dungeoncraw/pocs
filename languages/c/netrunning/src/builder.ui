<?xml version="1.0" encoding="UTF-8"?>
<interface>
    <object id="window" class="GtkWindow">
        <property name="title">Grid</property>
        <child>
            <object id="grid" class="GtkGrid">
                <child>
                    <object id="generateNet" class="GtkButton">
                        <property name="label">Generate Net</property>
                        <layout>
                            <property name="column">0</property>
                            <property name="row">0</property>
                        </layout>
                    </object>
                </child>
                <child>
                    <object id="generateDeck" class="GtkButton">
                        <property name="label">Generate Deck</property>
                        <layout>
                            <property name="column">0</property>
                            <property name="row">1</property>
                        </layout>
                    </object>
                </child>
                <child>
                    <object id="generateNetrunner" class="GtkButton">
                        <property name="label">Generate Netrunner</property>
                        <layout>
                            <property name="column">0</property>
                            <property name="row">2</property>
                        </layout>
                    </object>
                </child>
                <child>
                    <object id="quit" class="GtkButton">
                        <property name="label">Quit</property>
                        <layout>
                            <property name="column">0</property>
                            <property name="row">3</property>
                            <property name="column-span">2</property>
                        </layout>
                    </object>
                </child>
            </object>
        </child>
    </object>
</interface>